def anch(x,y,):
    a=len(x)
    if len(x)==len(y):

        if sorted(x)==sorted(y):
                print('The given strings are anagram')
        else:
                print('Its not an anagram')
    else:
        print('Invalid Input')





x=input('Enter string one : ')
y=input('Enter string two : ')
anch(x,y,)
